<Window x:Class="ADO_1.Views.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADO_1.Views"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="300">
    <StackPanel Orientation="Vertical">
        <Label HorizontalAlignment="Center" FontWeight="Bold" FontSize="20">
            Tech ADO.NET
        </Label>
        <Button x:Name="IntoButton" Click="IntoButton_Click" Margin="5 5" FontSize="16" Padding="0 2 0 4">
            Database initialization
        </Button>
    </StackPanel>
</Window>
